{
  "name": "YouTube Panel",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Show a floating panel with an embedded YouTube video.",
  "homepage_url": "https://youtube.com",
  "icons": {
    "16": "icons/16x16.png",
    "32": "icons/32x32.png",
    "64": "icons/64x64.png",
    "128": "icons/128x128.png",
    "256": "icons/256x256.png",
    "512": "icons/512x512.png"
  },
  "default_locale": "en",
  "background": {
    "scripts": [
      "src/background.js"
    ],
    "persistent": false
  },
  "page_action": {
    "default_title": "Open Panel",
    "default_icon": {
      "16": "icons/16x16.png",
      "32": "icons/32x32.png",
      "64": "icons/64x64.png",
      "128": "icons/128x128.png",
      "256": "icons/256x256.png",
      "512": "icons/512x512.png"
    }
  },
  "permissions": [
    "tabs"
  ]
}